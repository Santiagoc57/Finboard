{
  "name": "finboard-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:next": "node -e \"require('fs').rmSync('.next', { recursive: true, force: true }); console.log('Cleared .next cache');\"",
    "dev": "next dev",
    "dev:reset": "npm run clean:next && next dev",
    "build": "next build",
    "build:reset": "npm run clean:next && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.16.10",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.14",
    "jsdom": "24.1.3",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.2",
    "vitest": "2.1.8"
  }
}
